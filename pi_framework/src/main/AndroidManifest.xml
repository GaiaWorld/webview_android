<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.kuplay.pi_framework">
    <application>
        <activity
                android:name=".framework.WebViewActivity"
                android:screenOrientation="landscape"
                android:configChanges="orientation|keyboardHidden|screenSize"
                android:launchMode="singleTask"
                android:theme="@style/FullScreenWithoutAny">
        </activity>
        <activity
                android:name=".framework.NewWebViewActivity"
                android:launchMode="singleInstance"
                android:configChanges="orientation|keyboardHidden|screenSize"
                android:theme="@style/FullScreenWithoutAny" />
        <service
                android:name=".module.service.UpdateAppService"
                android:enabled="true"
                android:exported="true" />
        <service
                android:name=".module.service.RestartService"
                android:enabled="true"
                android:exported="true" />
    </application>
</manifest>


