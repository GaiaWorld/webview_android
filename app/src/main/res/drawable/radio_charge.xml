<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 选中状态-->
    <item android:drawable="@mipmap/charge_paytype_checked" android:state_checked="true"></item>

    <!-- 未选中-->
    <item android:state_checked="false">
        <shape android:shape="oval">
            <stroke android:width="1dp" android:color="@color/chargePayChooseBorder" />
            <solid android:color="@color/chargePayChooseBg" />
        </shape>
    </item>


</selector>